<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@drawable/ic_shown_password" />
    <item android:height="26dp" android:width="26dp">


        <rotate
            android:fromDegrees="160"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="0">
            <shape android:shape="line">
                <stroke
                    android:width="2dp"
                    android:color="#000" />


            </shape>

        </rotate>
    </item>
</layer-list>